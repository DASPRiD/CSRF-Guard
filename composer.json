{
    "name": "dasprid/csrf-guard",
    "description": "PSR-7 CSRF guard",
    "type": "library",
    "require": {
        "php": "^7.1",
        "psr/http-message": "^1.0",
        "dflydev/fig-cookies": "^1.0",
        "http-interop/http-middleware": "^0.4",
        "ramsey/uuid": "^3.6",
        "lcobucci/jwt": "^3.2",
        "cultuurnet/clock": "^1.0",
        "dasprid/treereader": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^2.8",
        "psr/container": "^1.0",
        "zendframework/zend-diactoros": "^1.3"
    },
    "suggest": {
        "psr/container": "for using the shipped factories"
    },
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Ben Scholzen 'DASPRiD'",
            "homepage": "https://dasprids.de/",
            "email": "mail@dasprids.de"
        }
    ],
    "keywords": [
        "csrf",
        "http",
        "psr",
        "psr-7",
        "psr-11"
    ],
    "autoload": {
        "psr-4": {
            "DASPRiD\\CsrfGuard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DASPRiD\\CsrfGuardTest\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "coveralls": "coveralls",
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    }
}
